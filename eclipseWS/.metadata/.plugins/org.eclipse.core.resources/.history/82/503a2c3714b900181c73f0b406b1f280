package jrJava.multi_threading_1;

public class Task {

	public static void main(String[] args){
		
		SquareSumTask sst = new SquareSumTask();
		SquareRootSumTask srst = new SquareRootSumTask();
		
		sst.doTask();
		srst.doTask();
		
		int sum = 0;
		for (int i = 0; i <=10; i++) {
			sum+=i;
			System.out.println("In Task: sum = " + sum);
		}
	}
	
}
