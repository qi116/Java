package jrJava.prep_objects_for_dataStructures_1;

public class Thing {
	private int a;
	public Thing(int a){
		this.a = a;
	}
	public int getA(){
		return a;
	}
	public boolean equals(Object b){
		
			Thing t = (Thing) b;
			if(t.a == a) return true;
		
		return false;
	}
	public String toString(){
		return "Thing";
	}
	public int hashCode(){
		return 3;
	}
}
